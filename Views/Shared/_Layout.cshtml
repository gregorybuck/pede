<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="DC.Title" content="Patricia Edgar and Don Edgar"/>
    <meta name="keywords" content="Edgar,Donald Edgar,Don Edgar,Patricia Edgar,family,sociology,new child,world summit,television,media,child,children,actf,marriage,mateship,bloodbath,work,children's TV,new technology,children's policy,education policy,early childhood,games,advertising,Star Wars,wide angle lens,Woomera,British missiles,Jindivik,optics,Australian Defence,Spasmodic dysphonia,speech therapy,voice disorders,dystonias,Botox"/>
    <meta name="DC.description" content="Patricia Edgar and Don Edgar"/>
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/ico" href="~/favicon.ico">
    @*<meta name="google-site-verification" content="@ViewBag.GoogleSiteVerification" />*@
    @Styles.Render("~/Content/css/css")
    @Styles.Render("~/Content/themes/base/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-6651318-2', 'auto');
        ga('send', 'pageview');
    </script>
</head>
<body>
    <div id="container">
        @*header .. Razor does not support SSIncludes *@
        <div id="header">
            <div class="imageDon"><img alt="Don Edgar" src="~/Content/images/img_don.jpg"/></div>
            <h1 class="headertext">Patricia Edgar &amp; Don Edgar</h1>
            <div class="imagePatricia"><img alt="Patricia Edgar" src="~/Content/images/img_tricia.jpg"/></div>
        </div>
	    @*topNAV menu*@
        <div class="topNav">
            @Html.Partial("~/Views/Shared/MainMenu.cshtml")
        </div>
        @*leftNAV menu*@
        <div id="leftNav">
            @Html.Partial("~/Views/Shared/LeftMenu.cshtml")
        </div>
	    @RenderBody()
        @*footer*@
        <div class="DividerDiv">
            <div class="topNav">
                @Html.Partial("~/Views/Shared/MainMenu.cshtml")
            </div>
        </div>
        
    </div>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
